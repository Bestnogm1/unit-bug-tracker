
<main> 
<%= title %>
<h1>this is the tickets page</h1> 
<a href="/tickets/new">new</a>



<table>
    <thead> 
      <tr>
        <th>assingedTo</th>
        <th>details</th>
        <th>Problems</th>
        <th>Severity</th>
        <th>ID</th>
        <th>detail</th>
      </tr>
    </thead>
    <tr>
      <% tickets.forEach((ticket)=>{ %>
        <td><%= ticket.assingedTo %> </td>
        <td><%= ticket.details  %> </td>
        <td><%= ticket.problems %> </td>  
        <td><%= ticket.severity %> </td>

        <td> <a href="tickes/details">Detail</a></td>

    </tr>
      <% }) %> 

</table>

</main>












